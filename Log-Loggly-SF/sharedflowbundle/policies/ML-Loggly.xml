<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<MessageLogging async="false" continueOnError="false" enabled="true" name="ML-Loggly">
    <DisplayName>ML-Loggly</DisplayName>
    <Syslog>
        <FormatMessage>true</FormatMessage>
        <Message variablePrefix="%" variableSuffix="#">[%private.logging_key#@41058 tag="apigee-edge-syslog" tag="%organization.name#" tag="%environment.name#" ] 
        { 
            "messageId":"%messageid#",
            "organizationName":"%organization.name#",
            "environmentName":"%environment.name#"
        }
        </Message>
        <Host>logs-00.loggly.com</Host>
        <Port>504</Port>
    </Syslog>
</MessageLogging>